{
    "name": "Read it",
    "description": "Read it",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "activeTab",
        "contextMenus",
        "https://www.facebook.com/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "img/get_started16.png",
        "32": "img/get_started32.png",
        "48": "img/get_started48.png",
        "128": "img/get_started128.png"
    },
    "browser_action": {
        "default_title": "Read it"
    }
}